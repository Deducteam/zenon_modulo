$sig abst_T () "Type"
$sig not_b ("bool") "bool"
$sig abst_equal ("abst_T" "abst_T") "bool"

$2 (A. ((x "bool") (<=> (Is_true (not_b x)) (-. (Is_true x)))))

$def (abst_different x y) (not_b (abst_equal x y))

$goal (A. ((x "abst_T") (A. ((y "abst_T")
         (=> (Is_true (abst_equal x y)) (-. (Is_true (abst_different x y))))))))
