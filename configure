bindir=/usr/local/bin

case "$1" in
  -bindir | --bindir)
     bindir=$2
     shift 2
  ;;
  -bindir=* | --bindir=*)
     bindir=`echo "$1" | sed 's/.*=//'`
     shift
  ;;
esac

{
  echo BINDIR=$bindir
} >.config_var
